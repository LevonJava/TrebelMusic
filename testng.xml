<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Testing TrebelMusic Application">
    <listeners>
        <listener class-name="com.trebel.util.MyListener"/>
    </listeners>
    <test name="Login Authentication">
        <classes>
            <class name="com.trebel.test.authentication.GetStarted"/>
            <class name="com.trebel.test.authentication.CreateAccount"/>
            <class name="com.trebel.test.authentication.Login"/>
        </classes>
    </test>
</suite>